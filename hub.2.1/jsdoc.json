{
	"plugins": [
		"plugins/markdown",
		"node_modules/better-docs/component",
		"node_modules/better-docs/category"
	],
	"recurseDepth": 10,
	"source": {
		"include": ["./components", "./pages", "./public", "./services"],
		"includePattern": ".+\\.js(doc|x)?$"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": [
			"category",
			"component"
		],
		"dictionaries": [
			"jsdoc",
			"closure"
		]
	},
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": false
	},
	"opts": {
		"template": "node_modules/better-docs"
	}
}